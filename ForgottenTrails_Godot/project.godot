printer_state="*res://project/scripts/autoloads/VNState.gd"
SignalBus="*res://project/scripts/autoloads/signal_bus.gd"
InputManager="*res://project/scripts/autoloads/input_manager.gd"
AudioManager="*res://project/scenes/audio_manager.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[dotnet]

project/assembly_name="ForgottenTrails"
project_build_enabled=true

[editor_plugins]

enabled=PackedStringArray("res://addons/GodotInk/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://editor/": "gray"
}


config_version=5

[application]

config/name="ForgottenTrails"
run/main_scene="res://project/main_menu/main_menu_scene.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://project/main_menu/ForgottenTrailsTitle_IMG.png"

[audio]

buses/default_bus_layout="res://project/game_settings/default_bus_layout.tres"

[autoload]

DataManager="*res://project/saveloading/data_manager.gd"
ConfigHandler="*res://project/game_settings/config_handler.gd"
SceneNavigator="*res://project/scene_navigator.gd"
printer_state="*res://project/VNState.gd"
SignalBus="*res://project/signal_bus.gd"
InputManager="*res://project/input_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[dotnet]
project_build_enabled=true

project/assembly_name="ForgottenTrails"

[editor_plugins]

enabled=PackedStringArray("res://addons/GodotInk/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://editor/": "gray",
